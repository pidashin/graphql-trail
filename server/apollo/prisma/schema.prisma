datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  trips    Trip[]
}

model Trip {
  id       Int    @id @default(autoincrement())
  launchId String

  user   User @relation(fields: [userId], references: [id])
  userId Int

  @@unique([launchId, userId])
}
